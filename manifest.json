{
  "manifest_version": 2,
  "name": "Tweet Report to YouTrack",
  "version": "0.1",
  "permissions": [
    "contextMenus",
    "storage",
    "https://youtrack.jetbrains.com/"
  ],
  "background": {
    "scripts": [
      "tweet-to-yt.js"
    ]
  },
  "icons": {
    "16": "youtrack-16.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
  }
}